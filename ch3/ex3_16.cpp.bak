		cout << i << endl;
	cout << "size:" << v5.size () << endl;
	for (auto i : v5)
		cout << i << endl;
	cout << "size:" << v6.size () << endl;
	for (auto i : v6)
		cout << i << endl;
	cout << "size:" << v7.size () << endl;
	for (auto i : v7)
		cout << i << endl;



	return 0;
}

